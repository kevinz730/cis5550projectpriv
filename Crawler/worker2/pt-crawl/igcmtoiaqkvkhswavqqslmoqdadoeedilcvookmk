<script type="text/javascript">
    $(document).ready(function() {

		$(".manual-table-not-sortable table th").addClass("{sorter: false}");


        //OW-300, Sort by Ordinals in Charts
        //
        //fix up the congress columns so they sort correctly
        $('.manual-table-sortable').each(function() {
            var $tbl = $(this);
            
            //get the congress column
            var col = $tbl.attr('data-congress-column');

            if (col) {
                //col--;
                console.log('here');
                var $tds = $tbl.find('tbody tr:nth-child(' + col + ')');

                $tbl.find('tbody tr').each(function() {
                    var $tr = $(this);
                    
                    var $td = $tr.find('td:nth-child(' + col + ')');
                    var val = parseInt($td.text());
                    $td.attr('sort', val);
                });
            }
        });



        try {
            
            //Sortable tables only
            $(".manual-table-sortable table").tablesorter({
			    widgets: ["zebra"],
                textExtraction: function(node) { 
                    // extract data from markup and return it  
                    var sort = $(node).attr("sort");

                    if (sort != undefined && sort != false) {
                        return sort;
                    } else {
                        return $(node).text();
                    }
                } 
            });

		    $(".manual-table-sortable table th").css({
			    cursor: "pointer"
		    });
        } catch(e) {
            //do nothing
        }
    });

</script>



    
    


